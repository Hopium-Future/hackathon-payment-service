{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "quotes": [
            "off",
            "double"
        ],
        "strict": "off",
        "semi": [
            "error",
            "never"
        ],
        "no-use-before-define": "off",
        "comma-dangle": [
            "error",
            "never"
        ],
        "space-before-function-paren": [
            "error",
            {
                "anonymous": "never",
                "named": "always"
            }
        ],
        "class-methods-use-this": "off",
        "object-curly-newline": [
            "error",
            {
                "multiline": true
            }
        ],
        "global-require": "off",
        "arrow-parens": [
            "error",
            "as-needed"
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-console": "off",
        "indent": [
            "warn",
            4
        ],
        "no-underscore-dangle": "off",
        "max-len": [
            "error",
            {
                "code": 200,
                "comments": 200,
                "ignoreUrls": true,
                "ignoreTemplateLiterals": true
            }
        ],
        "no-await-in-loop": "off",
        "no-plusplus": "off",
        "no-unused-vars": [
            "error",
            {
                "vars": "local",
                "args": "none"
            }
        ],
        "func-names": "off",
        "prefer-rest-params": "off",
        "camelcase": "off",
        "no-unused-expressions": "off",
        "no-sequences": "off",
        "no-prototype-builtins": "off",
        "no-restricted-syntax": "off",
        "no-bitwise": "off",
        "no-buffer-constructor": "off"
    },
    "globals": {
        "use": true,
        "Logger": true
    }
}
